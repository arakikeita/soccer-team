
 
 
 <div class='col-md-8 offset-md-1'>
      <h2>>チームページ</h2>
      <table class='table table-hover table-inverse'>
        <tr>
          <td><%= image_tag @team.image, class: 'team-image' if @team.image.attached? %></td>
          <td><%= @team.team_name %></td>
          <td><%= @team.name %></td>
          <td><%= @team.area_id %></td>
          <td><%= link_to 'カレンダー', calenders_path %></td>
          <td>
              <% @team.users.each do |member| %>
                <%= member.name %><br>
              <% end %>
          </td>
          
        </tr>
      </table>
    </div>